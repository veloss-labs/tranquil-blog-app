{
  "name": "tranquil-blog",
  "version": "0.0.1-beta.0",
  "description": "my own silent blog",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "dev:run": "run-s dev:node:*",
    "dev:node:cli": "node dist/cli.js --env local",
    "dev:node:start": "next",
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "fix": "run-p fix:*",
    "fix:types": "tsc --noEmit",
    "fix:eslint-fix": "next lint --fix",
    "fix:prettier-fix": "prettier --write '**/*.{js,jsx,md,ts,tsx}'",
    "cli:start": "tsup --entry.cli scripts/cli/index.ts --minify",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ci": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test"
  },
  "keywords": [
    "blog",
    "tranquil",
    "lalossol"
  ],
  "author": "",
  "license": "ISC",
